{
  "name": "server",
  "version": "1.0.0",
  "description": "server for application created with react-native",
  "main": "./dist/index.js",
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development tsc-watch --onFirstSuccess \"npm run serve-dist\" --onSuccess \"npm run serve-dist\"",
    "serve-dist": "tsc-alias && node dist/index.js",
    "build": "tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
